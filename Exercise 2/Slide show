<!DOCTYPE html>
<html>
<body>

<h1 id="myHeader">Slide show </h1>

<img id="slides" src="https://media-cldnry.s-nbcnews.com/image/upload/streams/2014/July/140710/1D274906334704-today-voice-impressions-140710-01.jpg" style="width:300px;height:300px;">

<button onclick="previous()">Previous</button>
<button onclick="next()">Next</button>




<script>
var currentphoto=0;
var maxphotos=6;
const photoids = ["https://media-cldnry.s-nbcnews.com/image/upload/streams/2014/July/140710/1D274906334704-today-voice-impressions-140710-01.jpg", "https://www.bmi.com/cache/images/news/2014/cantor_r_indie_770_436_70_s.jpg", "https://upload.wikimedia.org/wikipedia/commons/f/f3/Rob_Cantor1.jpg", "https://hiddeninthesand.com/wp-content/uploads/2014/09/rob-clones2-crop.jpg", "data:image/jpeg;base64,", "https://i.ytimg.com/vi/kymSZpfykBM/maxresdefault.jpg"];
function previous() {
	currentphoto=(currentphoto+maxphotos-1)%maxphotos;
    document.getElementById("slides").src = photoids[currentphoto];
}

function next() {
	currentphoto=(currentphoto+1)%maxphotos;
    document.getElementById("slides").src = photoids[currentphoto];
}
</script>

</body>
</html>

